<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".configFragment">

    <Button
        android:id="@+id/Colocar_etiqueta"
        android:layout_width="246dp"
        android:layout_height="80dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="200dp"
        android:text="Envie etiqueta" />

    <EditText
        android:id="@+id/etiqueta"
        android:layout_width="246dp"
        android:layout_height="80dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="60dp"
        android:hint="Coloque el nombre de la etiqueta" />

    <TextView
        android:id="@+id/Instrucciones"
        android:layout_width="246dp"
        android:layout_height="80dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="350dp"
        android:text="Coloque el nombre de la etiqueta y envie la etiqueta para ser procesada" />


    </FrameLayout>